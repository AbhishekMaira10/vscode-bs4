{
  "Bootstrap 4 Nav Stock": {
    "prefix": "!bsnav",
    "body": [
      "<nav class=\"navbar navbar-expand-lg navbar-light bg-light\">",
      "<ul class=\"nav $1\">",
      "<li class=\"nav-item\">",
      "<a class=\"nav-link active\" href=\"#\">Active</a>",
      "</li>",
      "<li class=\"nav-item\">",
      "<a class=\"nav-link\" href=\"#\">Link</a>",
      "</li>",
      "<li class=\"nav-item\">",
      "<a class=\"nav-link disabled\" href=\"#\">Disabled</a>",
      "</li>",
      "</ul>",
      "</nav>"
    ],
    "description": "Default Navigation for Bootstrap 4"
  },
  "Bootstrap 4 Nav Center": {
    "prefix": "!bsnavc",
    "body": "justify-content-center",
    "description": "Class to Align Navigation Center"
  },
  "Bootstrap 4 Nav Right": {
    "prefix": "!bsnavr",
    "body": "justify-content-end",
    "description": "Class to Align Navigation Right"
  },
  "Bootstrap 4 Nav Tabs": {
    "prefix": "!bsnavtab",
    "body": "nav-tabs",
    "description": "Class to Use Tab Style Navigation"
  },
  "Bootstrap 4 Nav Pills": {
    "prefix": "!bsnavpill",
    "body": "nav-pills",
    "description": "Class to Use Pill Style Navigation"
  },
  "Bootstrap 4 Sticky Nav": {
    "prefix": "!bsnavsticky",
    "body": [
      "<nav class=\"navbar navbar-expand-lg navbar-light bg-light fixed-top\">",
      "<ul class=\"nav $1\">",
      "<li class=\"nav-item\">",
      "<a class=\"nav-link active\" href=\"#\">Active</a>",
      "</li>",
      "<li class=\"nav-item\">",
      "<a class=\"nav-link\" href=\"#\">Link</a>",
      "</li>",
      "<li class=\"nav-item\">",
      "<a class=\"nav-link disabled\" href=\"#\">Disabled</a>",
      "</li>",
      "</ul>",
      "</nav>"
    ],
    "description": "Default Sticky Navigation for Bootstrap 4"
  },
  "Bootstrap 4 Nav Justify": {
    "prefix": "!bsnavj",
    "body": "nav-justified",
    "description": "Class to Justify Navigation"
  },
  "Bootstrap 4 Nav Fill": {
    "prefix": "!bsnavf",
    "body": "nav-fill",
    "description": "Class to Fill Navigation"
  },
  "Bootstrap 4 Nav Dropdown": {
    "prefix": "!bsnavdd",
    "body": [
      "<li class=\"nav-item dropdown\">",
      "<a class=\"nav-link dropdown-toggle\" data-toggle=\"dropdown\" href=\"#\" role=\"button\" aria-haspopup=\"true\" aria-expanded=\"false\">Dropdown</a>",
      "<div class=\"dropdown-menu\">",
      "<a class=\"dropdown-item\" href=\"#\">$1</a>",
      "<div class=\"dropdown-divider\"></div>",
      "<a class=\"dropdown-item\" href=\"#\">Separated link</a>",
      "</div>",
      "</li>"
    ],
    "description": "Default Dropdown Navigation"
  }
}